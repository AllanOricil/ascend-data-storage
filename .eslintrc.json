{
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true,
    "vitest-globals/env": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:vitest-globals/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "globals": {
    "RED": true,
    "$": "readonly",
    "document": "readonly",
    "window": "readonly",
    "Vue": "readonly"
  }
}
